{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 4],
    "no-new": "off",
    "no-restricted-globals": "off",
    "no-plusplus": "off",
    "no-await-in-loop": "off"
  },
  "globals": {
    "Highcharts": true
  }
}